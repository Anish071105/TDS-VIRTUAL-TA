{
  "version": 2,
  "builds": [
    {
      "src": "answer1.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "answer1.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "OPENAI_BASE_URL": "@openai-base-url",
    "GOOGLE_API_KEY": "@google-api-key"
  }
}
